        </activity>
        <activity android:name=".LeftFragment" />
        <activity android:name=".News" />
        <activity android:name=".NewsContentActivity" />
        <activity android:name=".NewsContentFragment" />
        <activity android:name=".NewsTitleFragment" />
        <activity android:name=".RightFragment" />
        <activity android:name=".RightFragment1" />
    </application>

</manifest>